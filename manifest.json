  "icons": {
    "64": "icons/batman-xxl.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://dinopark.k8s.dev.sso.allizom.org/",
    "https://dinopark.k8s.test.sso.allizom.org/",
    "https://people.mozilla.org/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/batman-xxl.png"
    },
    "default_title": "DinoParkLocalDev",
